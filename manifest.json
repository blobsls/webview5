{
    "manifest_version": 3,
    "name": "Global Dark Mode",
    "version": "1.0",
    "description": "darkmode",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}
